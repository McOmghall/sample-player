{
  "name": "sample-player",
  "version": "0.3.0",
  "description": "A simple audio buffer player",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run coverage && npm run docs && npm run disc && npm run dist",
    "test": "mocha",
    "docs": "documentation -f md > API.md",
    "dist": "mkdir -p dist && browserify lib/index.js | uglifyjs > dist/sample-player.min.js && ls -hall dist/",
    "coverage": "istanbul cover _mocha",
    "cover": "npm run coverage && open coverage/lcov-report/index.html",
    "prepublish": "npm test && npm run dist"
  },
  "repository": "https://github.com/danigb/smplr/tree/master/packages/sample-player",
  "keywords": [
    "audio",
    "buffer",
    "sample",
    "player",
    "webaudio"
  ],
  "author": "danigb <danigb@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danigb/smplr/issues"
  },
  "dependencies": {
    "adsr": "^1.0.0",
    "note-parser": "^1.1.0"
  },
  "devDependencies": {
    "audio-loader": "^0.4.0",
    "sinon": "^1.17.2",
    "web-audio-test-api": "^0.3.6"
  }
}
